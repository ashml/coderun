   "source": [
    "SELECT user_id, SUM(prev_time IS NULL OR time - prev_time > 30) AS cnt\n",
    "FROM (\n",
    "    SELECT user_id, time, LAG(time) OVER (PARTITION BY user_id ORDER BY time) as prev_time \n",
    "    FROM logs\n",
    "    ) t\n",
    "GROUP BY user_id\n",
    "ORDER BY user_id"
   ]
  }
