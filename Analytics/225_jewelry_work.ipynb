   "source": [
    "from scipy.stats import beta\n",
    "\n",
    "\n",
    "required_probability = 0.95\n",
    "threshold = 1000 / 1150\n",
    "K = 1\n",
    "\n",
    "while True:\n",
    "    cdf_value = beta.cdf(threshold, K + 1, 1)\n",
    "    if 1 - cdf_value >= required_probability:\n",
    "        break\n",
    "    K += 1\n",
    "\n",
    "print(K)"
   ]
  }
